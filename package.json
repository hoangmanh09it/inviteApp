  "scripts": {
    "start": "nodemon index.js --exec babel-node --presets es2015,stage-2",
    "lint": "node_modules/.bin/eslint ./ --fix",
    "test": "mocha --timeout 300s --require test/init.js --compilers js:babel-core/register --colors --reporter spec --recursive test",
    "pretest-coverage": "rm -Rf coverage",
    "test-coverage": "nyc npm test",
    "pretest-require-corverall": "npm run test-coverage",
    "test-require-corverall": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "predeploy": "npm test",
    "migrate": "babel-node bin/migrate.js",
    "rollback-migrate": "babel-node bin/migrate.js down"
  },
  "nyc": {
    "reporter": [
      "text",
      "lcov"
    ],
    "exclude": [
      ".eslintrc.js",
      "test/**",
      "libs/**"
    ]
